  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "files": [
      "**/*.spec.js",
      "!src-frontend/**/*.spec.js"
    ],
    "source": [
      "**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "verbose": true
