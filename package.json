    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src-frontend/mock/styleMock.js"
    },
    "testPathDirs": [
      "src-frontend"
    ],
    "testRegex": "\\.jest\\.spec\\.js"
